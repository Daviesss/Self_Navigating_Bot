<?xml version="1.0"?>
<launch>
  <arg name="no_static_map" default="true"/>
  <arg name="cmd_vel_topic" default="/cmd_vel" />
  <arg name="odom_topic"    default="/odom" />
  
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
  	
    <rosparam file="$(find navigator_bringup)/params2/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find navigator_bringup)/params2/costmap_common_params.yaml" command="load" ns="local_costmap" />
    
    <rosparam file="$(find navigator_bringup)/params2/map_nav_params/local_costmap_params.yaml"  command="load" />
    <rosparam file="$(find navigator_bringup)/params2/map_nav_params/global_costmap_params.yaml" command="load" />
    
    <rosparam file="$(find navigator_bringup)/params2/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find navigator_bringup)/params2/move_base_params.yaml"          command="load" />
    
    <param name="base_global_planner" value="navfn/NavfnROS"          type="string"  />
    <param name="base_local_planner"  value="base_local_planner/TrajectoryPlannerROS"/>
    
  </node>

</launch>
