<?xml version="1.0"?>
<!--
  Used to display URDF model in rviz
-->
<launch>
  <arg   name="model" />
  <param name="robot_description"     textfile="$(find navigator_bringup)/urdf/robotr_description.urdf" />
  <node  name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
  <node  name="rviz"                  pkg="rviz"                  type="rviz"  args="-d $(find navigator_bringup)/rviz/robot_mapping.rviz" />
</launch>